	"main": "server.js",
	"scripts": {
		"start": "npm run serve",
		"build": "npm run build-ts && npm run lint",
		"serve": "NODE_ENV=production node dist/server.js",
		"dev": "NODE_ENV=production nodemon dist/server.js",
